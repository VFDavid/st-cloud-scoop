{
  "crons": [
    {
      "path": "/api/cron/process-rss",
      "schedule": "30 20 * * *"
    },
    {
      "path": "/api/cron/send-final",
      "schedule": "55 4 * * *"
    },
    {
      "path": "/api/cron/import-metrics",
      "schedule": "0 6 * * *"
    },
    {
      "path": "/api/cron/health-check",
      "schedule": "*/15 8-22 * * *"
    }
  ],
  "functions": {
    "app/api/cron/process-rss/route.ts": {
      "maxDuration": 300
    },
    "app/api/cron/send-final/route.ts": {
      "maxDuration": 60
    },
    "app/api/cron/import-metrics/route.ts": {
      "maxDuration": 120
    },
    "app/api/cron/health-check/route.ts": {
      "maxDuration": 30
    }
  },
}